{
	"name": "Darkplusix",
	"include": "./dark_vs.json",
	"settings": [
		{
			"name": "CSS property value",
			"scope": [
				"keyword.operator.assignment"
			],
			"settings": {
				"foreground": "#8d2eb7"
			}
		},
		{
			"scope": [
				"constant.symbol",
				"punctuation.definition.other.constant.symbol",
				"group.static.class.name"
			],
			"settings": {
				"foreground": "#C586C0"
			}
		},
		{
			"scope": [
				"entity.name.function",
				"punctuation.definition.other.constant.symbol",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#4EC9B0"
			}
		},
		{
			"scope": [
				"punctuation.section.scope",
				"punctuation.separator.method"
			],
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": [
				"punctuation.section.function"
			],
			"settings": {
				"foreground": "#d16969"
			}
		},
		{
			"scope": [
				"punctuation.section.array"
			],
			"settings": {
				"foreground": "#f8a9b4"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"punctuation.embedded.section"
			],
			"settings": {
				"foreground": "#f8d10f",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"variable.other.readwrite.module",
				"module.variable.other.punctuation.readwrite.definition"

			],
			"settings": {
				"foreground": "#68EA52"
			}
		},
		{
			"scope": [
				"comment",
				"support.function.variable.quoted.single.heredoc",
				"comment.documentation"
			],
			"settings": {
				"foreground": "#808080",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
								"variable.other.constant"],
			"settings": {
				"foreground": "#9cdcfe",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#b5cea8"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#f44747",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword.other.special-method",											
				"round.accessor"],
			"settings": {
				"foreground": "#FF5722",
				"fontStyle": "bold"
			}
		},
		{
			"scope": ["support.function.variable.quoted.single",
								"embedded.keyword.control"
								],
			"settings": {
				"foreground": "#FF5722",
				"fontStyle": "italic"
			}
		}
	]
}
